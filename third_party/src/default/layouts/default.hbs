<!doctype html>
<html class="default no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{#ifCond model.name '==' project.name}}{{project.name}}{{else}}{{removeQuotes model.name}} |
        {{project.name}}{{/ifCond}}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="{{relativeURL "assets/css/main.css"}}">
</head>

<body>
<div class="main">
    {{> header}}
    <div class="container container-main">
        <div class="row">
            <div class="col-menu menu-sticky-wrap menu-highlight">
                {{#if mdPagesNavigation.children}}
                    <nav class="tsd-navigation outline primary">
                        <ul>
                            {{#each mdPagesNavigation.children}}
                                {{> navigation}}
                            {{/each}}
                        </ul>
                    </nav>
                {{/if}}
                {{#if settings.outline}}
                    <nav class="tsd-navigation outline primary">
                        <ul style="display:none">
                            {{json settings.outline.[0]}}
                        </ul>
                    </nav>
                {{/if}}
                <nav class="tsd-navigation root">
                    <ul>
                        {{! Globals }}
                        {{#each navigation.children}}
                            {{> toc.root}}
                        {{/each}}
                    </ul>
                </nav>
                <nav class="tsd-navigation">
                    <ul class="before-current">
                        {{#each toc.children}}
                            {{> toc.root}}
                        {{/each}}
                    </ul>
                </nav>
            </div>
            <div class="col-content">
                {{{contents}}}
                {{> footer}}
            </div>
        </div>
    </div>
</div>
<div class="overlay"></div>
<script src="{{relativeURL "assets/js/main.js"}}"></script>

{{> analytics}}

</body>

</html>
