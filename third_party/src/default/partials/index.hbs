{{#if groups}}
    <section class="tsd-panel-group tsd-index-group">
        <section class="tsd-panel tsd-index-panel">
            <div class="tsd-index-content">
                {{#each groups}}
                    <section class="tsd-index-section {{cssClasses}}">
                        {{#if categories}}
                            {{#each categories}}
                                <h3>{{title}}</h3>
                                <div class="tsd-index-list">
                                    {{#each children}}
                                        <div class="{{cssClasses}}"><a href="{{relativeURL url}}"
                                                                       class="tsd-kind-icon">{{#if name}}{{{removeQuotes
                                                name}}}{{else}}<em>{{{wbr kindString}}}</em>{{/if}}</a>
                                        </div>
                                    {{/each}}
                                </div>
                            {{/each}}
                        {{else}}
                            <h3>{{title}}</h3>
                            <div class="tsd-index-list">
                                {{#each children}}
                                    <div class="{{cssClasses}}"><a href="{{relativeURL url}}"
                                                                   class="tsd-kind-icon">{{#if name}}{{{removeQuotes
                                            name}}}{{else}}<em>{{{wbr kindString}}}</em>{{/if}}</a>
                                    </div>
                                {{/each}}
                            </div>
                        {{/if}}
                    </section>
                {{/each}}
            </div>
        </section>
    </section>
{{/if}}
